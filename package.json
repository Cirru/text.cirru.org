{
  "name": "calcit-workflow",
  "version": "0.1.0",
  "description": "ClojureScript workflow using Calcit Editor",
  "main": "index.js",
  "scripts": {
    "w-dl": "cr --emit-js --init-fn=app.dl/main! --reload-fn=app.dl/main! --emit-path=dl-out/",
    "bundle-main": "yarn esbuild dl.js --bundle --platform=node --outfile=dl-out/bundle.js",
    "c-dl": "cr --emit-js --init-fn=app.dl/main! --reload-fn=app.dl/main! --emit-path=dl-out/ -1 && yarn bundle-main"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mvc-works/calcit-workflow.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "devDependencies": {
    "axios": "^0.21.4",
    "bottom-tip": "^0.1.2",
    "esbuild": "^0.12.28",
    "vite": "^2.5.10"
  },
  "dependencies": {
    "@calcit/procs": "^0.4.35",
    "cirru-color": "^0.2.1-a1",
    "feather-icons": "^4.28.0",
    "highlight.js": "^11.2.0",
    "remarkable": "^2.0.1",
    "shortid": "^2.2.16"
  }
}
